# ===== DEV Profile - Local Development =====
# Bu dosyayı kopyalayıp application-dev.properties olarak kaydet ve değerleri doldur

# ===== MSSQL Database =====
spring.datasource.url=jdbc:sqlserver://localhost\\SQLEXPRESS:1433;databaseName=sprintmate;encrypt=true;trustServerCertificate=true
spring.datasource.username=your_username_here
spring.datasource.password=your_password_here
spring.datasource.driver-class-name=com.microsoft.sqlserver.jdbc.SQLServerDriver

# ===== GitHub OAuth =====
github.client.id=your_github_client_id
github.client.secret=your_github_client_secret

# ===== JWT =====
jwt.secret=your-super-secret-key-that-is-at-least-256-bits-long-for-hs256-algorithm
jwt.expiration=86400000

# ===== Development Settings =====
spring.jpa.show-sql=true
logging.level.org.hibernate.SQL=DEBUG
logging.level.com.yilmaz.sprintmate=DEBUG
